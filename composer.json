{
  "name" : "ipub/websockets-wamp",
  "type" : "library",
  "description" : "Extension for implementing WAMP protocol into ipub/websockets server",
  "keywords" : [
    "nette",
    "websockets",
    "wamp",
    "ipub",
    "ipublikuj",
    "framework",
    "tools"
  ],

  "homepage" : "https://github.com/iPublikuj/websockets-wamp",

  "license" : [
    "GPL-2.0",
    "GPL-3.0"
  ],

  "authors" : [
    {
      "name" : "iPublikuj:cms",
      "email" : "info@ipublikuj.eu",
      "homepage" : "https://www.ipublikuj.eu/"
    }
  ],

  "support" : {
    "email" : "support@ipublikuj.eu",
    "issues" : "https://github.com/iPublikuj/websockets-wamp/issues"
  },

  "extra" : {
    "ipub" : {
      "configuration" : {
        "extension" : "IPub\\WebSocketsWAMP\\DI\\WebSocketsWAMPExtension"
      }
    }
  },

  "require" : {
    "php" : ">=7.3.0",

    "psr/log" : "~1.0",

    "nette/di" : "~3.0",
    "nette/utils" : "~3.0",

    "symfony/serializer" : "^3.0 || ^4.0",

    "ipub/websockets" : "~1.4"
  },

  "require-dev" : {
    "nette/bootstrap" : "~3.0",
    "nette/mail" : "~3.0",
    "nette/robot-loader" : "~3.0",
    "nette/safe-stream" : "~2.3",
    "nette/tester" : "~2.3",

    "tracy/tracy" : "~2.4",

    "contributte/event-dispatcher" : "~0.5",
    "contributte/console" : "~0.6",

    "pds/skeleton" : "~1.0"
  },

  "autoload" : {
    "psr-0" : {
      "IPub\\WebSocketsWAMP\\" : "src/"
    }
  }
}
